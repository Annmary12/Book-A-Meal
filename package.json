{
  "name": "book-a-meal",
  "version": "1.0.0",
  "description": "Book-A-Meal is an application that allows customers to make food orders and helps the food vendor know what the customers want to eat. ",
  "main": "server/app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production babel-node server/app.js",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node -- server/app.js",
    "test": "npm run test:server",
    "pretest:server": "concurrently \"npm run lint:server\" \"npm run migrate:test && npm run seed:test\"",
    "test:server": "cross-env NODE_ENV=test nyc mocha server/tests",
    "migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "migrate:test": "sequelize --env test db:migrate:undo:all && sequelize --env test db:migrate",
    "seed": "sequelize db:seed:undo:all && sequelize db:seed:all",
    "seed:test": "sequelize --env test db:seed:all",
    "lint:server": "eslint \"server/**/*.js\" app.js --fix",
    "lint-test:server": "concurrently \"npm run lint:server\" \"npm run test:server\"",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otseobande/Book-A-Meal.git"
  },
  "author": "Obande Otse",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/otseobande/Book-A-Meal/issues"
  },
  "homepage": "https://github.com/otseobande/Book-A-Meal#readme",
  "dependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "bcrypt": "^2.0.1",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "express": "^4.16.3",
    "express-group-routes": "^1.1.0",
    "express-validation": "^1.0.2",
    "istanbul": "^0.4.5",
    "joi": "^13.2.0",
    "joi-phone-number": "^2.0.7",
    "jsonwebtoken": "^8.2.1",
    "mocha": "^5.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "moment": "^2.22.1",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.4",
    "nyc": "^11.7.1",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.6",
    "sequelize-cli": "^4.0.0",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.0.0",
    "sinon-express-mock": "^2.0.0",
    "swagger-ui-express": "^3.0.9"
  },
  "devDependencies": {}
}

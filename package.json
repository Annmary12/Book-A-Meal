{
  "name": "book-a-meal",
  "version": "1.0.0",
  "description": "Book-A-Meal is an application that allows customers to make food orders and helps the food vendor know what the customers want to eat. ",
  "main": "app.js",
  "scripts": {
    "test:server": "cross-env NODE_ENV=test nyc --reporter=html --all --reporter=text --require babel-core/register --require regenerator-runtime/runtime mocha server/tests --exit --recursive ",
    "start:dev": "nodemon --exec babel-node -- app.js",
    "lint:server": "eslint \"server/**/*.js\" app.js --fix",
    "lint:test:server": "concurrently \"npm run lint:server\" \"npm run test:server\"",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otseobande/Book-A-Meal.git"
  },
  "author": "Obande Otse",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/otseobande/Book-A-Meal/issues"
  },
  "homepage": "https://github.com/otseobande/Book-A-Meal#readme",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.1",
    "pg": "^7.4.1",
    "sequelize": "^4.37.6",
    "sequelize-cli": "^4.0.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "concurrently": "^3.5.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.7.1",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.0.0",
    "sinon-express-mock": "^2.0.0"
  }
}

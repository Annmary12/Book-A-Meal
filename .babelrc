{
	"presets": ["env", "react"],
	"plugins": [
		"transform-object-rest-spread",
		["transform-runtime", {
			"polyfill": false,
			"regenerator": true
		}],
		"add-module-exports",
		"transform-class-properties",
		"syntax-dynamic-import"
  ],
  "env": {
    "test": {
      "plugins": ["istanbul"]
    }
  }
}